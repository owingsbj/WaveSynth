<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="false" android:state_focused="false" android:state_selected="false">
		<nine-patch android:src="@drawable/tab_button_default" />
	</item>
	<item android:state_pressed="true">
		<nine-patch android:src="@drawable/tab_button_selected" />
	</item>
	<item android:state_focused="true">
		<nine-patch android:src="@drawable/tab_button_selected" />
	</item>
	<item android:state_selected="true">
		<nine-patch android:src="@drawable/tab_button_selected" />
	</item>
</selector>
